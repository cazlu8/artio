{
  "name": "engage-web-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "pretest": "npm i",
    "test": "cross-env NODE_ENV=development jest --forceExit",
    "test:watch": "cross-env NODE_ENV=development jest --watch  --forceExit",
    "test:cov": "cross-env NODE_ENV=development jest --coverage --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:ws": "artillery run ./test/websockets/loadtest.yml --output result.json ",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/bull": "^0.1.1",
    "@nestjs/common": "7.1.0",
    "@nestjs/config": "0.4.2",
    "@nestjs/core": "7.1.0",
    "@nestjs/platform-fastify": "7.1.0",
    "@nestjs/platform-socket.io": "^7.3.2",
    "@nestjs/swagger": "^4.5.8",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^6.3.4",
    "@nestjs/websockets": "^7.3.2",
    "aws-sdk": "^2.683.0",
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "bull": "^3.15.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "fastify-compress": "^2.0.1",
    "fastify-cookie": "^3.6.1",
    "fastify-cors": "^3.0.3",
    "fastify-csrf": "^1.0.3",
    "fastify-file-upload": "2.1.0",
    "fastify-healthcheck": "^2.3.1",
    "fastify-helmet": "^3.0.2",
    "fastify-rate-limit": "^3.0.1",
    "fastify-swagger": "^2.5.1",
    "firebase-admin": "^8.12.1",
    "gae-ayaml-env": "0.0.19",
    "group-by": "0.0.1",
    "install-me-maybe": "0.0.6",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "nestjs-redis": "^1.2.8",
    "pg": "^7.18.2",
    "promise.any": "^2.0.1",
    "redlock": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.5",
    "scramjet": "^4.34.0",
    "sharp": "^0.25.3",
    "short-uuid": "^3.1.1",
    "sleep": "^6.3.0",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.3.0",
    "twilio": "^3.46.0",
    "typeorm": "^0.2.25",
    "uuidv4": "^6.1.0",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^2.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.11.11",
    "@types/bull": "^3.14.0",
    "@types/express": "^4.17.6",
    "@types/jest": "25.1.2",
    "@types/node": "^13.13.15",
    "@types/socket.io": "^2.1.10",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-airbnb-typescript-prettier": "^1.4.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.5.4",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": "spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
