{
  "name": "engage-web-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "pretest": "npm i",
    "typescript": "^3.8.3",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch  --forceExit",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@google-cloud/logging": "^7.3.0",
    "@nestjs/common": "^7.0.8",
    "@nestjs/config": "^0.3.0",
    "@nestjs/core": "^7.0.8",
    "@nestjs/platform-fastify": "^7.0.8",
    "@nestjs/swagger": "^4.5.3",
    "@nestjs/typeorm": "^6.3.4",
    "axios": "^0.19.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "fastify-compress": "^2.0.1",
    "fastify-cookie": "^3.6.0",
    "fastify-cors": "^3.0.3",
    "fastify-csrf": "^1.0.3",
    "fastify-helmet": "^3.0.2",
    "fastify-rate-limit": "^3.0.1",
    "fastify-swagger": "^2.5.1",
    "firebase-admin": "^8.11.0",
    "gae-ayaml-env": "0.0.19",
    "group-by": "0.0.1",
    "install-me-maybe": "0.0.6",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.5",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.11.11",
    "@types/express": "^4.17.6",
    "@types/jest": "25.1.2",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-airbnb-typescript-prettier": "^1.4.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.4.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": "spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
